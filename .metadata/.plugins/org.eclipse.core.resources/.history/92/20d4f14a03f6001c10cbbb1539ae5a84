package Array;

public class bigg {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int max,number,NumberLessThan50=0;
		int a[]= {10,20,90,43,49,50,48,54,78};	
		number=50;
		for(int i=0;i<a.length;i++)
		{
			for(int j=1;j<a.length;j++)
			{
			if(a[i]<number)
			{
				 NumberLessThan50=a[i];
			}
			}
		}
		System.out.println("NumberSlightlyLessThan50is:- "+NumberLessThan50);
		
		

	}

}
