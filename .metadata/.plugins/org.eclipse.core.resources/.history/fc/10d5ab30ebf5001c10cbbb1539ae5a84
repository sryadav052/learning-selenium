package Array;

public class bigg {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int min;
		int a[]= {10,20,43,49,50,48,54,78};		
		min=a[0];
		int max=a[0];
		for(int i=1;i<a.length;i++)
		{
			if(a[i]<min)
			{
				min=a[i];
			}
		}
		System.out.println(+min);
		
		for(int i=1;i<a.length;i++)
		{
			if(a[i]<50) //
			{
				max=a[i];
				System.out.println(+max);
			}
		}
		System.out.println(+max);


	}

}
