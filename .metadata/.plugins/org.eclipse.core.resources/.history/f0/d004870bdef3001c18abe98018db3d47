package Basic;

import java.util.Scanner;

public class whleLoop {
	// while loop we use when we don't know about the  of loop 

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int n;
		System.out.println("Enter the conditional vale /n" );
		Scanner scn=new Scanner(System.in);
		n=scn.nextInt();
		 while(n>=0) {
			 System.out.println("Sanjeev yadav");
		 }

	}

}
