package PracticeQuestion;

import java.util.Scanner;

public class PrintVowelAndConsonant {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		char ch;
		System.out.println("Enter any character from alphabet");
		Scanner rScanner=new Scanner(System.in);
		ch=rScanner.next().charAt(0);
		
		if(ch=='a'|| ch=='e'|| ch=='i'|| ch=='o'|| ch=='u')
		{
			System.out.println("Enter characters is a vowel");
		}
		else  
			{
				System.out.println("Enter Character is a consonant");
			}
		

	}

}
